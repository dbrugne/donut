{
  "apps": [
    {
      "name": "web",
      "script": "app.js",
      "env": {
        "NODE_ENV": "development"
      },
      "exec_mode": "cluster",
      "instances": 2,
      "merge_logs": true,
      "cwd": "./web-server"
    },
    {
      "name": "master",
      "script": "app.js",
      "env": {
        "NODE_ENV": "development"
      },
      "args": [
        "type=master",
        "id=master-server-1",
        "host=127.0.0.1",
        "port=3005"
      ],
      "cwd": "./ws-server"
    },
    {
      "name": "chat-server-1",
      "script": "app.js",
      "env": {
        "NODE_ENV": "development"
      },
      "args": [
        "serverType=chat",
        "debug=32312",
        "id=chat-server-1",
        "host=127.0.0.1",
        "port=6050"
      ],
      "cwd": "./ws-server"
    },
    {
      "name": "connector-server-1",
      "script": "app.js",
      "env": {
        "NODE_ENV": "development"
      },
      "args": [
        "serverType=connector",
        "debug=32312",
        "id=connector-server-1",
        "host=127.0.0.1",
        "port=4050",
        "frontend=true",
        "clientPort=3050"
      ],
      "cwd": "./ws-server"
    }
  ]
}